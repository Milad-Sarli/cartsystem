{
    "name": "miladsarli/cartsystem",
    "description": "یک سیستم سبد خرید انعطاف پذیر برای اپلیکیشن های لاراول",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Milad Sarli",
            "email": "info@miladsarli.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "MiladSarli\\CartSystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiladSarli\\CartSystem\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MiladSarli\\CartSystem\\CartServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
